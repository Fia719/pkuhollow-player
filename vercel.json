{
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/api/treehole"
		}
	],
	"headers": [
		{
			"source": "/static/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "s-maxage=3600, stale-while-revalidate=30"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "Cloudflare-CDN-Cache-Control",
					"value": "max-age=3600"
				}
			]
		},
		{
			"source": "/js/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "s-maxage=3600, stale-while-revalidate=30"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "Cloudflare-CDN-Cache-Control",
					"value": "max-age=3600"
				}
			]
		},
		{
			"source": "/css/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "s-maxage=3600, stale-while-revalidate=30"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "Cloudflare-CDN-Cache-Control",
					"value": "max-age=3600"
				}
			]
		},
		{
			"source": "/img/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "s-maxage=86400, stale-while-revalidate=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=86400"
				},
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=86400"
				},
				{
					"key": "Cloudflare-CDN-Cache-Control",
					"value": "max-age=86400"
				}
			]
		}
	]
}